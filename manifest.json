{
  "name": "Swagify",
  "description": "Une extension qui s'occupe de bloquer les fallacieux traqueurs publicitaires.",
  "version": "1.0.1",
  "manifest_version": 3,
  "permissions": [
    "cookies",
    "tabs",
    "storage" 
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js", 
    "type": "module"
  },
  "action": {
    "default_popup": "main.html",
    "default_icon": "images/BLANK.jpg"
  }
}
